{
  "name": "oscars",
  "version": "1.0.0",
  "description": "looking at oscars data",
  "scripts": {
    "start": "budo index.js:dist/bundle.js --live --open",
    "postinstall": "cp node_modules/standard/eslintrc.json .eslintrc || true",
    "lint": "standard",
    "build": "browserify index.js | uglifyjs --compress --mangle > dist/bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rolyatmax/oscars.git"
  },
  "author": "Taylor Baldwin <taylorbaldwin@gmail.com> (https://tbaldw.in)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rolyatmax/oscars/issues"
  },
  "homepage": "https://github.com/rolyatmax/oscars#readme",
  "dependencies": {
    "budo": "^9.4.7",
    "color": "^1.0.3",
    "get-pixels": "^3.3.0",
    "get-rgba-palette": "^2.0.1",
    "promisify-node": "^0.4.0"
  },
  "devDependencies": {
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babelify": "^7.3.0",
    "browserify": "^14.1.0",
    "standard": "^8.6.0",
    "uglify-js": "^2.7.5"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ],
          "plugins": [
            "transform-object-rest-spread"
          ]
        }
      ]
    ]
  }
}
